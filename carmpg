    "from sklearn.model_selection import train_test_split"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 345,
   "id": "3b2eb646-aa7a-4cd7-b5e8-cf9b34158dda",
   "metadata": {},
   "outputs": [],
   "source": [
    "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.05, random_state=42)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 346,
   "id": "cdd772b3-1757-408c-8f97-d003444ab4a0",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Reg:\n",
    "    def __init__(self):\n",
    "        self.b = []\n",
    "\n",
    "    def fit(self, X, y):\n",
    "        self.b = np.dot(np.dot(np.linalg.inv(np.dot(X.T, X)), X.T), y)\n",
    "\n",
    "    def predict(self, X):\n",
    "        y_pred = []\n",
    "        for i in X.values:\n",
    "            y = 0\n",
    "            for j in range(len(self.b)):\n",
    "                y += self.b[j] * i[j]\n",
    "            y_pred.append(y)\n",
    "        return y_pred"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 347,
   "id": "5ac38922-614b-493f-843f-e4f6865df6c9",
   "metadata": {},
   "outputs": [],
   "source": [
    "def rmse(y_test, y_pred):\n",
    "    y_test = y_test.values\n",
    "    summ = 0\n",
    "    for i in range(0, len(y_pred)):\n",
    "        summ += (y_test[i] - y_pred[i]) ** 2\n",
    "    return np.sqrt(summ / len(y_pred))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 358,
   "id": "98748a69-772e-4f84-b257-91769053bf8f",
   "metadata": {},
   "outputs": [],
   "source": [
    "def R2(y_test, y_pred):\n",
    "    y_avg = y_test.mean()\n",
    "    sum_r1 = sum((y_test - y_pred) ** 2)\n",
    "    sum_r2 = sum((y_test - y_avg) ** 2)\n",
    "    return (1 - sum_r1 / sum_r2) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 349,
   "id": "abdd2df0-2d59-451c-882f-715d3497b081",
   "metadata": {},
   "outputs": [],
   "source": [
    "model = Reg()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 350,
   "id": "6c9a0733-feb1-4e77-a4ed-bd3b8354a688",
   "metadata": {},
   "outputs": [],
   "source": [
